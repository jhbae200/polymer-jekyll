{
  "entrypoint": "index.html",
  "shell": "src/my-app/index.html",
  "fragments": [
    "src/my-home/index.html",
    "src/my-post/index.html",
    "src/my-view404.html",
    "src/my-network-warning.html"
  ],
  "sources": [
    "_layouts/about.html",
    "src/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "bundle": true,
      "html": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "js": {
        "minify": true,
        "compile": true
      },
      "addServiceWorker": true,
      "passingPattern": [
        "_layouts/*",
        "_posts/*",
        "paginate/*",
        "images/**/*",
        "tags.html",
        "manifest.json",
        "sitemap.*",
        "feed.xml",
        "about.md"
      ]
    }
  ]
}
